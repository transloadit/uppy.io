{
  "name": "uppy-io",
  "version": "0.0.0",
  "private": true,
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.js": "eslint --fix --cache",
    "*.md": [
      "remark --silently-ignore -foq",
      "eslint --fix"
    ]
  },
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "prepare": "husky install",
    "lint": "eslint . --cache && remark -f -q ."
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-beta.22",
    "@docusaurus/preset-classic": "^2.0.0-beta.22",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-beta.22",
    "@tsconfig/docusaurus": "^1.0.4",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-mdx": "^2.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "remark-cli": "^11.0.0",
    "remark-frontmatter": "^4.0.1",
    "remark-preset-lint-consistent": "^5.1.1",
    "remark-preset-lint-markdown-style-guide": "^5.1.2",
    "remark-preset-lint-recommended": "^6.1.2",
    "remark-preset-prettier": "^1.0.1",
    "typescript": "^4.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
